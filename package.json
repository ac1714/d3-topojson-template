{
  "name": "d3-topojson-template",
  "version": "2.1.1",
  "description": "Boilerplate that maps a data set to a D3 map of the US.",
  "main": "",
  "private": true,
  "scripts": {
    "dev": "npm run watch & npm start",
    "watch": "npm run watch-js & npm run watch-sass",
    "watch-js": "watchify js/main.js -o js/bundle.js -dv",
    "watch-sass": "nodemon -e scss -x \"npm run compile-sass\"",
    "compile-sass": "./node_modules/.bin/node-sass -o css sass/main.scss bundle.css",
    "start": "node server.js"
  },
  "repository": "https://github.com/jonchretien/d3-template-topojson/",
  "author": {
    "name": "Jon Chretien",
    "url": "http://jonchretien.com/"
  },
  "license": "MIT",
  "devDependencies": {
    "browserify": "~8.1.1",
    "watchify": "~2.2.1",
    "queue-async": "~1.0.7",
    "d3": "~3.5.3",
    "topojson": "~1.6.18",
    "microtemplates": "~0.1.0",
    "node-sass": "~2.0.0-beta",
    "uglifyjs": "~2.3.6",
    "jshint": "~2.6.0",
    "express": "~4.11.1",
    "nodemon": "~1.3.2"
  }
}
